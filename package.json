{
  "name": "docfork",
  "version": "1.3.0",
  "description": "Up-to-date code documentation for AI agents.",
  "mcpName": "io.github.docfork/docfork-mcp",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc --watch",
    "inspect": "npx @modelcontextprotocol/inspector node dist/index.js",
    "dev": "concurrently \"npm run watch\" \"npm run inspect\"",
    "build": "tsc && chmod 755 dist/index.js",
    "start": "MCP_TRANSPORT=streamable-http node dist/index.js",
    "start:stdio": "MCP_TRANSPORT=stdio node dist/index.js",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "lint:check": "eslint \"**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docfork/docfork-mcp.git"
  },
  "keywords": [
    "modelcontextprotocol",
    "mcp",
    "docfork"
  ],
  "author": "Docfork",
  "license": "MIT",
  "bin": {
    "docfork": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/docfork/docfork-mcp/issues"
  },
  "homepage": "https://github.com/docfork/docfork-mcp#readme",
  "dependencies": {
    "@modelcontextprotocol/inspector": "0.18.0",
    "@modelcontextprotocol/sdk": "1.25.2",
    "dotenv": "^17.2.3",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "25.0.8",
    "@typescript-eslint/eslint-plugin": "8.52.0",
    "@typescript-eslint/parser": "8.53.0",
    "concurrently": "^9.2.1",
    "eslint": "9.39.2",
    "globals": "17.0.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}
