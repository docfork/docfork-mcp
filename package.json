{
  "name": "docfork",
  "version": "1.0.0",
  "description": "Up-to-date documentation to 9,000+ libraries for devs and AI agents.",
  "mcpName": "com.docfork/docfork-mcp",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc --watch",
    "inspect": "npx @modelcontextprotocol/inspector node dist/index.js",
    "dev": "concurrently \"npm run watch\" \"npm run inspect\"",
    "build": "tsc && chmod 755 dist/index.js",
    "start": "MCP_TRANSPORT=streamable-http node dist/index.js",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docfork/docfork-mcp.git"
  },
  "keywords": [
    "modelcontextprotocol",
    "mcp",
    "docfork"
  ],
  "author": "Docfork",
  "license": "MIT",
  "bin": {
    "docfork": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/docfork/docfork-mcp/issues"
  },
  "homepage": "https://github.com/docfork/docfork-mcp#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "dotenv": "^17.2.1",
    "escape-html": "^1.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/dotenv": "^8.2.3",
    "@types/escape-html": "^1.0.4",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
}
