{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.docfork/docfork",
  "description": "Up-to-date Docs for AI Agents.",
  "icons": [
    {
      "src": "https://mcp.docfork.com/icon.svg",
      "mimeType": "image/svg+xml"
    }
  ],
  "repository": {
    "url": "https://github.com/docfork/docfork",
    "source": "github"
  },
  "version": "1.3.4",
  "remotes": [
    {
      "type": "streamable-http",
      "url": "https://mcp.docfork.com/mcp",
      "headers": [
        {
          "name": "DOCFORK_API_KEY",
          "description": "API key for authentication",
          "isRequired": false,
          "isSecret": true
        },
        {
          "name": "DOCFORK_CABINET",
          "description": "optional cabinet scope",
          "isRequired": false,
          "isSecret": false
        }
      ]
    }
  ],
  "packages": [
    {
      "registryType": "npm",
      "identifier": "docfork",
      "version": "1.3.4",
      "transport": {
        "type": "stdio"
      }
    }
  ]
}
